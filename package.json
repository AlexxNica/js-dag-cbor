{
  "name": "@ipld/dag-cbor",
  "version": "0.0.0",
  "description": "JS implementation of dag-cbor",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "prepublishOnly": "npm run build",
    "lint": "standard",
    "test:cjs": "npm run build && mocha dist/test/test-*.cjs",
    "test:node": "hundreds mocha test/test-*.js",
    "test:browser": "polendina --cleanup test/test-*.js",
    "test": "npm run lint && npm run test:node && npm run test:browser && npm run test:cjs",
    "coverage": "c8 --reporter=html mocha test/test-*.js && npx st -d coverage -p 8080"
  },
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipld/js-ipld-dag-cbor.git"
  },
  "keywords": [
    "IPFS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipld/js-ipld-dag-cbor/issues"
  },
  "homepage": "https://github.com/ipld/js-ipld-dag-cbor",
  "dependencies": {
    "borc": "^2.1.2",
    "is-circular": "^1.0.2"
  },
  "devDependencies": {
    "garbage": "0.0.0",
    "globby": "^11.0.1",
    "hundreds": "0.0.7",
    "mocha": "^8.0.1",
    "multiformats": "1.1.11",
    "polendina": "^1.0.0",
    "rollup": "^2.18.0",
    "standard": "^14.3.4"
  },
  "directories": {
    "test": "test"
  },
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (https://www.mikealrogers.com/)"
}
